{
    "name": "peidgnad/peidmin",
    "description": "A lightweight, powerful and beauty admin interface for Laravel.",
    "homepage": "https://github.com/peidgnad/peidmin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DiepPK",
            "email": "peidgnad@gmail.com",
            "homepage": "https://peidgnad.github.io",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.7",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Peidgnad\\Peidmin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Peidgnad\\Peidmin\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Peidgnad\\Peidmin\\Providers\\PeidminServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
